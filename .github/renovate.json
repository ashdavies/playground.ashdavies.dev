{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "hashicorp/terraform",
      "depTypeTemplate": "terraform_version",
      "extractVersionTemplate": "v(?<version>.*)",
      "fileMatch": "^\\.github/workflows/.*\\.yml",
      "matchStrings": [
        "terraform_version: (?<currentValue>.*)"
      ],
      "versioningTemplate": "hashicorp"
    }
  ],
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  "gitAuthor": "Playground Manager <126197455+playground-manager[bot]@users.noreply.github.com>",
  "ignoreUnstable": true,
  "packageRules": [
    {
      "matchDatasources": [
        "maven"
      ],
      "registryUrls": [
        "https://repo.maven.apache.org/maven2/",
        "https://dl.google.com/dl/android/maven2/",
        "https://plugins.gradle.org/m2/"
      ]
    },
    {
      "matchPackageNames": [
        "actions/checkout"
      ],
      "extends": [
        "schedule:monthly"
      ]
    },
    {
      "groupName": "Compose Multiplatform",
      "matchPackagePrefixes": [
        "org.jetbrains.compose",
        "org.jetbrains.kotlin."
      ]
    },
    {
      "matchPackageNames": [
        "renovatebot/github-action"
      ],
      "extends": [
        "schedule:monthly"
      ]
    }
  ],
  "platformCommit": true,
  "repositories": [
    "ashdavies/playground.ashdavies.dev"
  ],
  "useBaseBranchConfig": "merge",
  "username": "playground-manager[bot]"
}
