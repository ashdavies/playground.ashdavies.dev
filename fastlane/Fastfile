default_platform(:android)

platform :android do
  lane :publish_to_play_store do
    upload_to_play_store(
      track: "internal",
      aab: "lanyard-app/build/outputs/bundle/release/lanyard-app-release.aab",
      mapping: "lanyard-app/build/outputs/mapping/release/mapping.txt",
      skip_upload_images: true,
    )
  end
end
